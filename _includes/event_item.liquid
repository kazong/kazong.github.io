<section class="unit text-center event-ac">
    <article class="wrapper text-center">
        <h2 class="section-headline">{{ gamesunit.title }}</h2>
        <div class="slider-container">
            <ul class="game-preview">
                {% for game in site.data[page.language].games-event_em2016 %}
                <li class="preview-item">
                    {% include game_teaser.liquid %}
                </li>
                {% endfor %}
            </ul>
        </div>
    </article>
</section>