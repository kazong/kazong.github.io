{% assign homepage = site.data[page.language].homepage %}
{% assign herounit = homepage.herounit %}
{% assign screenshots = homepage.screenshots %}
{% assign challengeunit = homepage.challengeunit %}

<section class="hero-unit unit">
    <canvas id="pixie" class="bg-anim"></canvas>
    <article class="wrapper section-one">
        <div class="section-textbox left">
            <div class="section-txt left">
                <p class="section1-headline">{{ herounit.headline }}</p>
                <p class="section1-sub">{{ herounit.kicker }}</p>
            </div>

            <div class="download-btn-container inline">
                <a href="https://play.google.com/apps/testing/com.kazong.game">
                    <img class="download-btn play" src="{{ page.base_dir }}images/btn-store-play.png" alt="{{ herounit.download_labels.android }}"/>
                </a>
                <a href="https://docs.google.com/forms/d/15TkQSSuOy80yCTO8K1IXWiP-H5dWBGkmcHHHm3gNVH8/viewform?c=0&w=1">
                    <img class="download-btn" src="{{ page.base_dir }}images/btn-store-apple.png" alt="{{ herounit.download_labels.ios }}"/>
                </a>
            </div>
        </div>
        <div class="lazy__container">
            <img class="main-device-img low-res" src="data:image/png;base64,"/>
            <img class="main-device-img lazy" data-original="{{ page.base_dir }}images/{{ screenshots.main.path }}" alt="{{ screenshots.main.label }}" />
        </div>
        <div class="lazy__container">
            <img class="main-device-img img-right low-res" src="data:image/png;base64,"/>
            <img class="main-device-img img-right lazy" data-original="{{ page.base_dir }}images/{{ screenshots.challenge.path }}" alt="{{ screenshots.challenge.label }}" />
        </div>
    </article>
</section>

<section class="bg-purple unit text-center challenge-section">
    <article class="wrapper text-center">
        <h2 class="section-headline">{{ challengeunit.titel }}</h2>
        <div class="section-one challenge-wrapper">
            <div class="image-box">
                <div class="lazy__container">
                    <img class="main-device-img low-res" src="data:image/png;base64,"/>
                    <img class="main-device-img lazy" data-original="{{ page.base_dir }}images/{{ screenshots.selector.path }}" alt="{{ screenshots.selector.label }}" />
                </div>
                <div class="lazy__container">
                    <img class="main-device-img img-right low-res" src="data:image/png;base64,"/>
                    <img class="main-device-img img-right lazy" data-original="{{ page.base_dir }}images/{{ screenshots.quiz_cut.path }}" alt="{{ screenshots.quiz_cut.label }}" />
                </div>
            </div>
            <div class="section-textbox right">
                <div class="section-txt right">
                    <p class="section1-headline">{{ challengeunit.headline }}</p>
                    <p class="section1-sub">{{ challengeunit.kicker }}</p>
                    <ul class="section1-sub sub-features">
                        {% for feature in challengeunit.features %}
                        <li class="sub-feature-item">{{ feature }}</li>
                        {% endfor %}
                    </ul>
                </div>
            </div>
        </div>
    </article>
</section>

<section class="bg-responsive unit text-center">
    <article class="wrapper text-center">
        <h2 class="section-headline">{{ homepage.games_label }}</h2>
        <div class="slider-container">
            <ul class="game-preview">
                {% for game in site.data[page.language].games %}
                <li class="preview-item">
                    {% include game_teaser.liquid %}
                </li>
                {% endfor %}
            </ul>
        </div>
    </article>
</section>