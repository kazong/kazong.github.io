{% assign event_2016_xmas = site.data[page.language].event_2016_xmas %}
{% assign eventunit = event_2016_xmas.eventunit %}
{% assign prizeunit = event_2016_xmas.prizeunit %}
{% assign screenshots = event_2016_xmas.screenshots %}
{% assign descriptionunit = event_2016_xmas.descriptionunit %}
{% assign gamesunit = event_2016_xmas.eventgamesunit %}
{% assign mentions = event_2016_xmas.event_prunit %}

<section class="event-unit unit xmas">
    <img class="event-wallpaper" src="../images/event/xmas2016/xmas-wallpaper.jpg"/>
    <article class="section-textbox">
        <div>
            <p class="event-headline">{{ eventunit.headline }}</p>
            <div class="section1-sub">
                <div class="prize-intro">
                    <div class="intro">{{ eventunit.kicker }}</div>
                    <div class="prize">
                        <div class="prize-item">
                            <img class="prize-img first" src="../images/event/xmas2016/amazon40.png">
                            <div class="prize-title">{{eventunit.prize.prize1}}</div>
                        </div>
                        <div class="prize-item">
                            <img class="prize-img second" src="../images/event/xmas2016/amazon20.png">
                            <div class="prize-title">{{eventunit.prize.prize2}}</div>
                        </div>
                        <div class="prize-item">
                            <img class="prize-img third" src="../images/event/xmas2016/amazon10.png">
                            <div class="prize-title">{{eventunit.prize.prize3}}</div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="download-btn-container">
                <div class="goToStore-txt middle">
                    <span class="playnow">{{eventunit.download.play}}</span>
                    <span class="code-intro">{{eventunit.download.now}}</span>
                </div>
                <div class="goToStore-btn">
                    <a data-category="goToStore-xmas2016" href="https://play.google.com/store/apps/details?id=com.kazong.game">
                        <img class="download-btn play" src="{{ page.base_dir }}images/btn/btn-store-play.png" alt="{{ herounit.download_labels.android }}"/>
                    </a>
                    <a data-category="goToStore-xmas2016" href="https://itunes.apple.com/{{ page.language }}/app/id1082430396?mt=8&ign-mpt=uo%3D4">
                        <img class="download-btn" src="{{ page.base_dir }}images/btn/btn-store-apple.png" alt="{{ herounit.download_labels.ios }}"/>
                    </a>
                </div>
            </div>
        </div>
    </article>
</section>

<section class="bg-purple unit challenge-section event-descr xmas">
    <article class="wrapper text-center">
        <h2 class="section-headline">{{ descriptionunit.titel }}</h2>
        <div class="duration">{{ descriptionunit.duration-th }}: {{ descriptionunit.duration }}</div>
        <div class="section-one challenge-wrapper">
            <div class="image-box">
                <div class="lazy__container">
                    <img class="main-device-img low-res" src="data:image/png;base64,"/>
                    <img class="main-device-img lazy" data-original="{{ page.base_dir }}images/{{ screenshots.challenge.path }}" alt="{{ screenshots.challenge.label }}" />
                </div>
                <div class="lazy__container">
                    <img class="main-device-img img-right low-res" src="data:image/png;base64,"/>
                    <img class="main-device-img img-right lazy" data-original="{{ page.base_dir }}images/{{ screenshots.game.path }}" alt="{{ screenshots.game.label }}" />

                </div>
            </div>
            <div class="section-textbox right">
                <div class="section-txt right">
                    <p class="section1-headline">{{ descriptionunit.event }}</p>
                    <p class="section1-sub">{{ descriptionunit.kicker }}</p>
                    <ul class="section1-sub sub-features">
                        {% for feature in descriptionunit.features %}
                        <li class="sub-feature-item">{{ feature }}</li>
                        {% endfor %}
                    </ul>
                </div>
            </div>
        </div>
    </article>
</section>

